{
  "name": "nestjs-prisma-client-starter",
  "version": "0.0.1",
  "description": "NestJS Prisma Client Starter Project",
  "author": "Marc Stammerjohann",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "Prisma",
    "Prisma Client",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daotl/nestjs-prisma-client-example.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/nestjs-prisma-client-example/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "build:e2e": "nest build -p tsconfig.e2e-test.json",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "prisma db seed --preview-feature",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "prepare": "husky install"
  },
  "prettier": "@daotl/prettier-config",
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "2.0.2",
    "@nestjs/common": "8.0.6",
    "@nestjs/config": "1.0.1",
    "@nestjs/core": "8.0.6",
    "@nestjs/graphql": "8.0.2",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-fastify": "8.0.6",
    "@nestjs/swagger": "5.0.9",
    "@prisma/client": "2.29.1",
    "apollo-server-core": "3.0.0-alpha.4",
    "apollo-server-fastify": "3.0.0-alpha.4",
    "bcrypt": "5.0.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "env-to-class": "0.1.3",
    "fastify": "3.20.2",
    "fastify-helmet": "5.3.2",
    "fastify-swagger": "4.9.0",
    "graphql": "15.5.1",
    "graphql-subscriptions": "1.2.1",
    "helmet": "4.6.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "subscriptions-transport-ws": "0.9.19",
    "swagger-ui-express": "4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/cz-commitlint": "^13.1.0",
    "@daotl/eslint-config": "^0.1.5",
    "@daotl/tsconfig": "^0.0.4",
    "@nestjs/cli": "8.1.1",
    "@nestjs/testing": "8.0.6",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/express": "4.17.12",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "chance": "1.1.7",
    "commitizen": "^4.2.4",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "^7.0.1",
    "jest": "27.0.6",
    "lint-staged": "^11.1.2",
    "patch-package": "^6.4.7",
    "prettier": "2.3.2",
    "prisma": "2.29.1",
    "prisma-dbml-generator": "0.7.0",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
